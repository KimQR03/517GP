#include <iostream>
using namespace std;

int main()
{
    int t;
    cin >> t;

    while (t > 0)
    {
        int n;
        cin >> n;

        int index = 0;
        while (n > 0)
        {
            if (n % 2 == 1)
                cout << index << " ";
            n /= 2;
            index++;
        }
        t--;
    }

}
