#include <iostream>
#include <string>
using namespace std;

int main()
{
    int a, b;
    cin >> a;
    string word;

    
    for (int i = 0; i < a; i++)
    {
        cin >> b >> word;
        
        for (int c = 0; c < word.length(); c++)
        {
            if (b-1 != c)
                cout << word[c];
        }
    
        cout << endl;
    }
}
